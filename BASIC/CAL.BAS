10 CLS
20 INPUT "ENTER 2 NUMBERS:", A, B
30 PRINT "1.ADD"
40 PRINT "2.SUBTRACT"
50 PRINT "3.MULTIPLY"
60 PRINT "4.DIVIDE"
70 INPUT "ENTER CHOICE:", C
80 ON C GOTO 90, 100, 110, 120
90 PRINT "ADDITION OF 2 NUMBERS ="; A + B: GOTO 130
100 PRINT "SUBTRACTION OF 2 NUMBERS ="; A - B: GOTO 130
110 PRINT "MULTIPLICATION OF 2 NUMBERS ="; A * B: GOTO 130
120 PRINT "DIVISION OF 2 NUMBERS ="; A / B: GOTO 130
130 END

